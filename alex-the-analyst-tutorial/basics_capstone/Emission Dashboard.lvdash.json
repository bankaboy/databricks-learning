{
  "datasets": [
    {
      "name": "d1110d0b",
      "displayName": "Location Data",
      "queryLines": [
        "select\r\n",
        "  latitude,\r\n",
        "  longitude,\r\n",
        "  `GHG emissions mtons CO2e` as emissions\r\n",
        "from \r\n",
        "  emissions.default.emissions_data"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "78776a87",
      "displayName": "Emissions Per Person",
      "queryLines": [
        "select\r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  cast(replace(`GHG emissions mtons CO2e`, ',', '') as double) / cast(population as double) as emissions_per_person\r\n",
        "from \r\n",
        "  emissions.default.emissions_data\r\n",
        "order by \r\n",
        "  emissions_per_person desc"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "12d39a42",
      "displayName": "Emissions Per State",
      "queryLines": [
        "select\r\n",
        "  state_abbr,\r\n",
        "  sum(cast(replace(`GHG emissions mtons CO2e`, ',', '') as double)) as total_emissions\r\n",
        "from \r\n",
        "  emissions.default.emissions_data\r\n",
        "group by \r\n",
        "  state_abbr\r\n",
        "order by\r\n",
        "  total_emissions desc\r\n",
        "limit 10"
      ]
    },
    {
      "name": "ae08a773",
      "displayName": "Emission Per County Top 10",
      "queryLines": [
        "select\r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  cast(replace(`GHG emissions mtons CO2e`, ',', '') as double) as total_emisions\r\n",
        "from \r\n",
        "  emissions.default.emissions_data\r\n",
        "order by \r\n",
        "  total_emisions desc\r\n",
        "limit 10"
      ]
    }
  ],
