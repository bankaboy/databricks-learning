      "displayName": "Product_Sales_Desc",
      "queryLines": [
        "SELECT \r\n",
        "    product, \r\n",
        "    sum(totalPrice)\r\n",
        "FROM \r\n",
        "    samples.bakehouse.sales_transactions\r\n",
        "GROUP BY \r\n",
        "    product\r\n",
        "ORDER BY \r\n",
        "    sum(totalPrice) DESC"
      ]
    }
